    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "rewire": "^5.0.0",
    "serverless-export-env": "^1.4.0",
    "serverless-iam-roles-per-function": "^2.0.2"
